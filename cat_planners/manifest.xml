<package>
  <description brief="cat_planners">

     Constraint-Aware Teleop planners.

  </description>
  <author>Adam Leeper</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/cat_planners</url>
  <depend package="roscpp"/>
  <depend package="urdf"/>
  <depend package="srdf"/>
  <depend package="moveit_core"/>
  <depend package="pluginlib"/>
  <depend package="tf_conversions"/>

  <export>
    <cpp cflags="-I${prefix}/include `rosboost-cfg --cflags`" lflags="-Wl,-rpath,${prefix}/lib -L${prefix}/lib -lcat_planners -lcvxsolvers `rosboost-cfg --lflags thread`"/>
    <planning_interface plugin="${prefix}/lib/plugin_description.xml"/>
  </export>

</package>


